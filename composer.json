{
    "name": "tedbreedigital/prismic-laravel",
    "description": "Laravel 5.6 Implementation Of Prismic PHP API",
    "keywords": ["Prismic", "API", "Laravel", "Tedbree Digital", "Content Management Systems", "CMS", "Headless CMS"],
    "type": "library",
    "license": "MIT",
    "homepage": "http://opensource.tedbreedigital.com/prismic-laravel",
    "authors": [{
        "name": "Ogunmoye Damilola",
        "email": "dami@ogunmoye.com",
        "homepage": "http://dami.ogunmoye.com",
        "role": "Developer"
    }],
    "support": {
        "source": "https://github.com/dammyammy/prismic-laravel",
        "issues": "https://github.com/dammyammy/prismic-laravel/issues"
    },
    "require": {
        "php": ">=7.1.3",
        "prismic/php-sdk": "4.0.0-beta"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "laravel/framework": "~5.6.13",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "TedbreeDigital\\Prismic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TedbreeDigital\\Prismic\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "TedbreeDigital\\Prismic\\ServiceProvider"
            ],
            "aliases": {
                "Prismic": "TedbreeDigital\\Prismic\\Facade"
            }
        }
    }
}
